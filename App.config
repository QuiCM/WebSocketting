<configuration>
  <system.diagnostics>
    <sources>
      <source name="WebSocket"
              switchName="SourceSwitch"
              switchType="System.Diagnostics.SourceSwitch" >
        <listeners>
          <add name="console"
               type="System.Diagnostics.ConsoleTraceListener">
            <filter type="System.Diagnostics.EventTypeFilter"
              initializeData="All"/>
          </add>
          <remove name = "Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SourceSwitch" value="All" />
    </switches>
  </system.diagnostics>
</configuration>
